{
  "name": "@ui5/webcomponents-fiori",
  "version": "2.19.0-rc.0",
  "description": "UI5 Web Components: webcomponents.fiori",
  "ui5": {
    "webComponentsPackage": true
  },
  "type": "module",
  "author": "SAP SE (https://www.sap.com)",
  "license": "Apache-2.0",
  "customElements": "dist/custom-elements.json",
  "keywords": [
    "openui5",
    "sapui5",
    "ui5"
  ],
  "exports": {
    "./dist/ssr-zxing.js": {
      "browser": "./dist/ssr-zxing.js",
      "node": "./dist/ssr-zxing-shim.js",
      "default": "./dist/ssr-zxing.js"
    },
    "./src/*": "./src/*",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json",
    "./bundle.esm.js": "./bundle.esm.js",
    "./bundle.common.js": "./bundle.common.js",
    "./bundle.scoped.esm.js": "./bundle.scoped.esm.js",
    "./*": "./dist/*"
  },
  "scripts": {
    "clean": "wc-dev clean",
    "lint": "wc-dev lint",
    "lint:scope": "wc-dev scope.lint",
    "start": "wc-dev start",
    "watch": "wc-dev watch",
    "build": "wc-dev build",
    "generate": "wc-dev generate",
    "generateAPI": "wc-dev generateAPI",
    "bundle": "wc-dev build.bundle",
    "test": "yarn test:cypress",
    "test:ssr": "node -e \"import('./test/ssr/component-imports.js')\"",
    "test:cypress": "wc-dev test-cy-ci",
    "test:cypress:single": "npx cypress run --component --browser chrome --spec",
    "test:cypress:open": "wc-dev test-cy-open",
    "create-ui5-element": "wc-create-ui5-element",
    "prepublishOnly": "tsc -b"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UI5/webcomponents.git",
    "directory": "packages/fiori"
  },
  "dependencies": {
    "@ui5/webcomponents": "2.19.0-rc.0",
    "@ui5/webcomponents-base": "2.19.0-rc.0",
    "@ui5/webcomponents-icons": "2.19.0-rc.0",
    "@ui5/webcomponents-theming": "2.19.0-rc.0",
    "@zxing/library": "^0.21.3"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.10.10",
    "@ui5/cypress-internal": "0.1.0",
    "@ui5/webcomponents-tools": "2.19.0-rc.0",
    "cypress": "^15.3.0",
    "lit": "^2.0.0",
    "vite": "5.4.21",
    "yarn": "^1.22.22"
  }
}
