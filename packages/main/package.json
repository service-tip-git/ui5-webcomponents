{
  "name": "@ui5/webcomponents",
  "version": "2.18.0-rc.0",
  "description": "UI5 Web Components: webcomponents.main",
  "ui5": {
    "webComponentsPackage": true
  },
  "type": "module",
  "author": "SAP SE (https://www.sap.com)",
  "license": "Apache-2.0",
  "customElements": "dist/custom-elements.json",
  "keywords": [
    "openui5",
    "sapui5",
    "ui5"
  ],
  "scripts": {
    "clean": "wc-dev clean",
    "lint": "wc-dev lint",
    "lint:scope": "wc-dev scope.lint",
    "start": "wc-dev start",
    "watch": "wc-dev watch",
    "generate": "wc-dev generate",
    "generateAPI": "wc-dev generateAPI",
    "build": "wc-dev build",
    "bundle": "wc-dev build.bundle",
    "test": "yarn test:cypress",
    "test:cypress": "wc-dev test-cy-ci",
    "test:cypress:suite-1": "wc-dev test-cy-ci-suite-1",
    "test:cypress:suite-2": "wc-dev test-cy-ci-suite-2",
    "test:cypress:suite-3": "wc-dev test-cy-ci-suite-3",
    "test:cypress:suite-4": "wc-dev test-cy-ci-suite-4",
    "test:cypress:open": "wc-dev test-cy-open",
    "test:cypress:single": "npx cypress run --component --browser chrome --spec",
    "test:vitest": "yarn vitest run",
    "test:ssr": "node -e \"import('./test/ssr/component-imports.js')\"",
    "create-ui5-element": "wc-create-ui5-element",
    "prepublishOnly": "tsc -b"
  },
  "exports": {
    "./src/*": "./src/*",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json",
    "./bundle.esm.js": "./bundle.esm.js",
    "./bundle.common.js": "./bundle.common.js",
    "./bundle.scoped.esm.js": "./bundle.scoped.esm.js",
    "./*": "./dist/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UI5/webcomponents.git",
    "directory": "packages/main"
  },
  "dependencies": {
    "@ui5/webcomponents-base": "2.18.0-rc.0",
    "@ui5/webcomponents-icons": "2.18.0-rc.0",
    "@ui5/webcomponents-icons-business-suite": "2.18.0-rc.0",
    "@ui5/webcomponents-icons-tnt": "2.18.0-rc.0",
    "@ui5/webcomponents-localization": "2.18.0-rc.0",
    "@ui5/webcomponents-theming": "2.18.0-rc.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.10.10",
    "@ui5/cypress-internal": "0.1.0",
    "@ui5/webcomponents-tools": "2.18.0-rc.0",
    "cypress": "^15.3.0",
    "jsdom": "^26.0.0",
    "lit": "^2.0.0",
    "vite": "5.4.21",
    "vitest": "^3.0.2"
  }
}
